ated":{"line":47,"column":24},"source":"scripts/viz5.js","original":{"line":41,"column":36}},{"generated":{"line":47,"column":25},"source":"scripts/viz5.js","original":{"line":41,"column":37}},{"generated":{"line":47,"column":26},"source":"scripts/viz5.js","original":{"line":41,"column":36}},{"generated":{"line":47,"column":27},"source":"scripts/viz5.js","original":{"line":41,"column":34}},{"generated":{"line":47,"column":28},"source":"scripts/viz5.js","original":{"line":41,"column":21}},{"generated":{"line":48,"column":0},"source":"scripts/viz5.js","original":{"line":41,"column":21}},{"generated":{"line":48,"column":3},"source":"scripts/viz5.js","original":{"line":29,"column":2}},{"generated":{"line":48,"column":5},"source":"scripts/viz5.js","original":{"line":26,"column":46}},{"generated":{"line":48,"column":6},"source":"scripts/viz5.js","original":{"line":42,"column":4}},{"generated":{"line":49,"column":0},"source":"scripts/viz5.js","original":{"line":43,"column":4}},{"generated":{"line":50,"column":0},"source":"scripts/viz5.js","original":{"line":44,"column":1}}],"sources":{"scripts/viz5.js":"/**\n * Updates the domain and range of the scale for the y axis\n *\n * @param {*} yScale The scale for the y axis\n * @param {string[]} neighborhoodNames The names of the neighborhoods\n * @param {number} height The height of the diagram\n */\n export function updateYScale (yScale, viz5Data, height) {\n  var totals = [];\n  viz5Data.forEach(line => {\n    var typeTotal = line['2015'] + line['2016'] + line['2017'] + line['2018'] + line['2019'] + line['2020'] + line['2021']\n    totals.push(typeTotal)\n  });\n  var maxValue = d3.max(totals)\n  yScale.domain([0, maxValue]).range([height, 0])\n}\n\n/**\n * Draws the bars inside the groups\n *\n * @param {*} y The graph's y scale\n * @param {*} x The x scale to use to position the rectangles in the groups\n * @param {*} color The color scale for the bars\n * @param {*} tip The tooltip to show when each bar is hovered and hide when it's not\n */\n export function drawBars (y, x, color, data) { //todo: 1 seule fonction pr tt graphs? il y a juste les catégories qui changent la donne.\n  var stackedData = d3.stack().keys(['2015', '2016', '2017', '2018', '2019', '2020', '2021'])(data)\n\n  d3.select('#graph-g')\n    .selectAll()\n    .data(stackedData)\n    .join('g')\n    .classed('series', true)\n    .attr('fill', function (d) { return color(d.key)})\n    .selectAll('rect')\n    .data((d)=>d)\n    .join('rect')\n    .attr('width', x.bandwidth())\n    .attr('x', function (d) { return x(d.data.type) })\n    .attr('y', function (d) { return y(d[1])})\n    .attr('height', d => y(d[0])-y(d[1])) \n    // .on('mouseover', tip.show)\n    // .on('mouseout', tip.hide)\n}\n"},"lineCount":null}},"error":null,"hash":"a2738a31491323f939b002f354c57e9a","cacheData":{"env":{}}}